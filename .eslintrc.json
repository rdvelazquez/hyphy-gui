{
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": [1, "never"],
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "react/react-in-jsx-scope": 1,
    "no-unused-vars": [1, {"vars":"all", "args":"none"}],
    "no-self-assign": 1,
    "no-unreachable": 1,
    "no-debugger": 1,
    "no-console":1,
    "indent": [1, 2]
  },
  "plugins": [
    "react"
  ]
}
